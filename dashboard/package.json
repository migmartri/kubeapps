{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/js-yaml": "^3.10.1",
    "@types/json-schema": "^6.0.1",
    "@types/moxios": "^0.4.8",
    "@types/pako": "^1.0.0",
    "@types/qs": "^6.5.1",
    "@types/react-jsonschema-form": "^1.0.4",
    "@types/react-select": "^1.2.6",
    "@types/ws": "^6.0.0",
    "axios": "^0.17.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-enzyme": "6.0.2",
    "js-yaml": "^3.10.0",
    "moniker-native": "^0.1.6",
    "open-iconic": "^1.1.1",
    "protobufjs": "^6.8.4",
    "qs": "^6.5.2",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-ace": "^5.9.0",
    "react-dom": "^16.2.0",
    "react-feather": "^1.0.8",
    "react-jsonschema-form": "^1.0.3",
    "react-lazylog": "^2.1.0",
    "react-markdown": "^3.1.4",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts-ts": "^2.17.0",
    "react-select": "^1.2.1",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.2.0",
    "ts-jest": "^22.0.1",
    "typesafe-actions": "^1.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js":
      "BROWSER=none REACT_APP_KUBEAPPS_NS=${TELEPRESENCE_CONTAINER_NAMESPACE} react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "yarn run build-css && react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "tslint-check": "tslint-config-prettier-check ./tslint.ci.json",
    "precommit": "lint-staged",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "lint-js": "tslint -c ./tslint.ci.json 'src/**/*.{ts,tsx}'",
    "lint-js-fix": "tslint -c ./tslint.ci.json --fix 'src/**/*.{ts,tsx}'",
    "lint-css": "stylelint --config ./.stylelintrc.json 'src/**/*.scss'",
    "lint-css-fix": "stylelint --config ./.stylelintrc.json --fix 'src/**/*.scss'",
    "lint": "npm-run-all lint-js lint-css"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["yarn run lint-js-fix", "prettier --write", "git add"],
    "*.scss": ["yarn run lint-css-fix", "prettier --write", "git add"],
    "*.{js,json,md}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.6",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.0.0",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-modal": "^3.1.1",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.20",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.11",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-mock-store": "^1.0.0",
    "husky": "0.14.3",
    "jest-context": "^2.1.0",
    "jest-enzyme": "6.0.2",
    "lint-staged": "^6.1.0",
    "mock-socket": "^8.0.2",
    "moxios": "^0.4.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.10.2",
    "react-svg-loader": "^2.1.0",
    "redux-mock-store": "^1.5.3",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*", "!src/**/*.d.ts"]
  }
}
